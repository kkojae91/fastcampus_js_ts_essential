= (store.currentPage - 1) * 10; i < store.currentPage * 10; i++) {\n    newsList.push(`\n    <li>\n      <a href=\"#/show/${newsFeed[i].id}\">${newsFeed[i].title} (${newsFeed[i].comments_count})</a>\n    </li>\n    `);\n  }\n  newsList.push(\"</ul>\");\n  newsList.push(`\n    <div>\n      <a href='#/page/${store.currentPage > 1 ? store.currentPage - 1 : 1}'>이전 페이지</a>\n      <a href='#/page/${store.currentPage < newsFeedLength ? store.currentPage + 1 : 3}'>다음 페이지</a>\n    </div>\n  `);\n  container.innerHTML = newsList.join(\"\");\n}\n\nfunction newsDetail() {\n  // substr(startIndex, length), substring(startIndex, endIndex), slice(startIndex, endIndex) 문자열 잘라내는 방법!\n  const id = location.hash.substr(7);\n  const newsContent = getData(CONTENT_URL.replace(\"@id\", id));\n\n  container.innerHTML = `\n    <h1>${newsContent.title}</h1>\n    <div>\n      <a href=\"#/page/${store.currentPage}\">목록으로</a>\n    </div>\n  `;\n}\n\nfunction router() {\n  const routePath = location.hash;\n\n  if (routePath === \"\") {\n    newsFeed();\n  } else if (routePath.indexOf(\"#/page/\") >= 0) {\n    store.currentPage = Number(routePath.substr(7));\n    newsFeed();\n  } else {\n    newsDetail();\n  }\n}\n\nwindow.addEventListener(\"hashchange\", router);\nrouter();\n"},"lineCount":null}},"error":null,"hash":"3a0585cf55172838d673e6278aeb8903","cacheData":{"env":{}}}